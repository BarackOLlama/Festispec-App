<Grid Background="WhiteSmoke">
        <Button Content="Customer Management" Command="{Binding OpenCustomerManagement}" HorizontalAlignment="Left" Margin="230,281,0,0" VerticalAlignment="Top" Width="150" Height="30"/>
    </Grid>
</Window>
